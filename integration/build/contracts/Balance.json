  "source": "pragma solidity ^0.4.17;\n\ncontract Balance {\n    uint public amount;\n    address public member;\n    \n    // Get balance\n    function getBalance() public returns (uint) {\n      amount = 199;\n      return 201;\n    }\n    \n    // Set member address\n    function setMember() public {\n      member = msg.sender;\n    }\n}",
